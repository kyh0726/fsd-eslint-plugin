{
  "name": "@yh-kim/eslint-plugin-fsd",
  "version": "0.1.11",
  "description": "ESLint plugin for Feature-Sliced Design architecture",
  "main": "lib/index.js",
  "exports": {
    ".": {
      "require": "./lib/index.js",
      "import": "./lib/index.js"
    },
    "./package.json": "./package.json"
  },
  "scripts": {
    "test": "mocha tests/**/*.js",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "format": "prettier --write \"**/*.{js,json,md}\"",
    "format:check": "prettier --check \"**/*.{js,json,md}\"",
    "version:patch": "pnpm version patch --no-git-tag-version",
    "prepare": "husky install"
  },
  "keywords": [
    "eslint",
    "eslintplugin",
    "eslint-plugin",
    "fsd",
    "feature-sliced-design",
    "architecture"
  ],
  "author": "",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/kyh0726/fsd-eslint-plugin"
  },
  "bugs": {
    "url": ""
  },
  "homepage": "",
  "engines": {
    "node": ">=14.0.0"
  },
  "peerDependencies": {
    "eslint": ">=7.0.0"
  },
  "devDependencies": {
    "eslint": "^8.57.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.1.3",
    "husky": "^8.0.3",
    "lint-staged": "^15.2.0",
    "mocha": "^10.3.0",
    "prettier": "^3.2.5"
  },
  "lint-staged": {
    "*.js": [
      "eslint --fix",
      "prettier --write"
    ],
    "*.{json,md}": [
      "prettier --write"
    ]
  },
  "files": [
    "lib",
    "README.md",
    "LICENSE"
  ]
}
